<Window x:Class="ImageSynchronizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageSynchronizer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel LastChildFill="True">
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" Margin="10 50">
            <TextBlock Text="Click To Process" Padding="10"></TextBlock>
            <Button Content="Process" Click="HandleProcessCommand" Width="180" Height="35"></Button>
        </StackPanel>
        <StackPanel DockPanel.Dock="Top"></StackPanel>
    </DockPanel>
</Window>
